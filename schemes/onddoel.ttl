	skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-5.3-222731821 rdf:type skos:Concept ;
	skos:definition "5.3 De leerlingen kunnen soorten van eenvoudige hedendaagse audiovisuele opnamen en weergavetoestellen (informatiedragers) aanwijzen, benoemen en ze creatief bedienen."@nl ;
	skos:hiddenLabel "5.3"@nl ;
	skos:inScheme onddoel:_scheme ;
	skos:prefLabel "basis-lager-5.3-222731821"@nl ;
	skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-muzische-vorming>, <http://ilearn.ilabt.imec.be/vocab/curr2/s-media>, <https://w3id.org/onderwijs-vlaanderen/id/structuur/lager-onderwijs> ;
	skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-5.3-3058276357 rdf:type skos:Concept ;
	skos:definition "5.3 De leerlingen ervaren dat bezig zijn met wiskunde een actief en een constructief proces is dat kan groeien en uitbreiden als gevolg van eigen denk- en leeractiviteiten; ze ontwikkelen bijgevolg de opvatting dat alle leerlingen wiskundige bekwaamheid kunnen verwerven die kan leiden naar studies en beroepen waarin wiskunde aan bod komt."@nl ;
	skos:hiddenLabel "5.3"@nl ;
	skos:inScheme onddoel:_scheme ;
	skos:prefLabel "basis-lager-5.3-3058276357"@nl ;
	skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>, <http://ilearn.ilabt.imec.be/vocab/curr2/s-getallen>, <http://ilearn.ilabt.imec.be/vocab/curr2/s-meetkunde>, <http://ilearn.ilabt.imec.be/vocab/curr2/s-meten>, <http://ilearn.ilabt.imec.be/vocab/curr2/s-stratprobv>, <https://w3id.org/onderwijs-vlaanderen/id/structuur/lager-onderwijs> ;
	skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-5.3-903632096 rdf:type skos:Concept ;
	skos:definition "5.3 De leerlingen kunnen bij de eindtermen luisteren, lezen, spreken en schrijven de volgende strategien inzetten: tijdens het luisteren, lezen, spreken en schrijven hun aandacht behouden voor het bereiken van het doel"@nl ;
	skos:hiddenLabel "5.3"@nl ;
	skos:inScheme onddoel:_scheme ;
