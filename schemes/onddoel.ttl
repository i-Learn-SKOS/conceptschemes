	skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-6.2-4076767426 rdf:type skos:Concept ;
	skos:definition "6.2 De leerlingen kunnen zonder vooroordelen naar kunst kijken en luisteren."@nl ;
	skos:hiddenLabel "6.2"@nl ;
	skos:inScheme onddoel:_scheme ;
	skos:prefLabel "basis-lager-6.2-4076767426"@nl ;
