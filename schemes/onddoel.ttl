	skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-6.4-4244845683 rdf:type skos:Concept ;
	skos:definition "6.4 De leerlingen kunnen vertrouwen op hun eigen expressiemogelijkheden en durven hun creatieve uitingen tonen."@nl ;
	skos:hiddenLabel "6.4"@nl ;
	skos:inScheme onddoel:_scheme ;
	skos:prefLabel "basis-lager-6.4-4244845683"@nl ;
	skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-muzische-vorming>, <http://ilearn.ilabt.imec.be/vocab/curr2/s-beeld>, <http://ilearn.ilabt.imec.be/vocab/curr2/s-beweging>, <http://ilearn.ilabt.imec.be/vocab/curr2/s-drama>, <http://ilearn.ilabt.imec.be/vocab/curr2/s-media>, <http://ilearn.ilabt.imec.be/vocab/curr2/s-muziek>, <https://w3id.org/onderwijs-vlaanderen/id/structuur/lager-onderwijs> ;
	skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-6.4-969536599 rdf:type skos:Concept ;
	skos:definition "6.4 Met het oog op doeltreffende communicatie kunnen de leerlingen in voor hen relevante en concrete taalgebruikssituaties op hun niveau reflecteren op:\n- het gebruik van standaardtaal, regionale en sociale taalvariteiten; \n- het gebruik van in hun omgeving voorkomende talen; \n- normen, houdingen, vooroordelen en rolgedrag via taal; \n- taalgedragsconventies; \n- de gevolgen van hun taalgedrag voor anderen en henzelf; \n- talige aspecten van cultuuruitingen in hun omgeving."@nl ;
	skos:hiddenLabel "6.4"@nl ;
	skos:inScheme onddoel:_scheme ;
	skos:prefLabel "basis-lager-6.4-969536599"@nl ;
	skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-nederlands>, <http://ilearn.ilabt.imec.be/vocab/curr2/s-taalbeschouwing>, <https://w3id.org/onderwijs-vlaanderen/id/structuur/lager-onderwijs> ;
	skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-6.5-2609996583 rdf:type skos:Concept ;
	skos:definition "6.5 De leerlingen kunnen respect betonen voor uitingen van leeftijdgenoten, behorend tot eigen en andere culturen."@nl ;
	skos:hiddenLabel "6.5"@nl ;
	skos:inScheme onddoel:_scheme ;
