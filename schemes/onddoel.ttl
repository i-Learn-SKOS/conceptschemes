	skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-4.6-1130682997 rdf:type skos:Concept ;
	skos:definition "4.6 De leerlingen kunnen het inoefenen, de voorbereiding, het aanwenden van de lichaamstaal en het uitvoeren (vertoning), door henzelf en anderen, kritisch bespreken."@nl ;
	skos:hiddenLabel "4.6"@nl ;
	skos:inScheme onddoel:_scheme ;
	skos:prefLabel "basis-lager-4.6-1130682997"@nl ;
	skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-muzische-vorming>, <http://ilearn.ilabt.imec.be/vocab/curr2/s-beweging>, <https://w3id.org/onderwijs-vlaanderen/id/structuur/lager-onderwijs> ;
	skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-4.6-1529420394 rdf:type skos:Concept ;
	skos:definition "4.6. De leerlingen hebben een voorstelling van de kaart van Vlaanderen en van Belgi zodat ze in een praktische toepassingssituatie de gemeenschappen, de provincies en de provinciehoofdplaatsen kunnen aanwijzen."@nl ;
	skos:hiddenLabel "4.6."@nl ;
	skos:inScheme onddoel:_scheme ;
	skos:prefLabel "basis-lager-4.6-1529420394"@nl ;
	skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-mens-en-maatschappij>, <http://ilearn.ilabt.imec.be/vocab/curr2/s-ruimte>, <http://ilearn.ilabt.imec.be/vocab/curr2/t-orientatie-en-kaartvaardigheid>, <https://w3id.org/onderwijs-vlaanderen/id/structuur/lager-onderwijs> ;
	skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-4.6-2174594800 rdf:type skos:Concept ;
	skos:definition "4.6 De leerlingen kunnen (verwerkingsniveau = structureren): schriftelijk antwoorden op vragen over verwerkte inhouden."@nl ;
	skos:hiddenLabel "4.6"@nl ;
	skos:inScheme onddoel:_scheme ;
	skos:prefLabel "basis-lager-4.6-2174594800"@nl ;
	skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-nederlands>, <http://ilearn.ilabt.imec.be/vocab/curr2/s-schrijven>, <https://w3id.org/onderwijs-vlaanderen/id/structuur/lager-onderwijs> ;
	skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-4.7-1178665988 rdf:type skos:Concept ;
	skos:definition "4.7. De leerlingen kunnen aan de hand van een concreet voorbeeld het verschil tussen beleefde en absolute afstand illustreren."@nl ;
	skos:hiddenLabel "4.7."@nl ;
	skos:inScheme onddoel:_scheme ;
	skos:prefLabel "basis-lager-4.7-1178665988"@nl ;
	skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-mens-en-maatschappij>, <http://ilearn.ilabt.imec.be/vocab/curr2/s-ruimte>, <http://ilearn.ilabt.imec.be/vocab/curr2/t-ruimtebeleving>, <https://w3id.org/onderwijs-vlaanderen/id/structuur/lager-onderwijs> ;
	skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-4.7-3745598549 rdf:type skos:Concept ;
	skos:definition "4.7 De leerlingen kunnen voor het realiseren van bovenstaande eindtermen bovendien:\n hun teksten verzorgen rekening houdende met handschrift en lay-out \n spellingsafspraken en -regels toepassen in verband met het schrijven van\n woorden met vast woordbeeld:\n  klankzuivere woorden\n hoogfrequente niet-klankzuivere woorden\n woorden met veranderlijk woordbeeld (regelwoorden):\n werkwoorden\n klinker in open/gesloten lettergreep\n verdubbeling medeklinker\n niet-klankzuivere eindletter\n hoofdletters\n interpunctietekens . , ? ! :"@nl ;
	skos:hiddenLabel "4.7"@nl ;
	skos:inScheme onddoel:_scheme ;
	skos:prefLabel "basis-lager-4.7-3745598549"@nl ;
	skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-nederlands>, <http://ilearn.ilabt.imec.be/vocab/curr2/s-schrijven>, <https://w3id.org/onderwijs-vlaanderen/id/structuur/lager-onderwijs> ;
	skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-4.8-2316503193 rdf:type skos:Concept ;
	skos:definition "4.8 De leerlingen ontwikkelen bij het realiseren van de eindtermen voor spreken, luisteren, lezen en schrijven de volgende attitudes:\n- luister-, spreek-, lees- en schrijfbereidheid;\n- plezier in luisteren, spreken, lezen en schrijven;\n- bereidheid tot nadenken over het eigen luister-, spreek-, lees- en schrijfgedrag;\n- bereidheid tot het naleven van luister-, spreek-, lees-, en schrijfconventies;\n- weerbaarheid."@nl ;
	skos:hiddenLabel "4.8"@nl ;
	skos:inScheme onddoel:_scheme ;
	skos:prefLabel "basis-lager-4.8-2316503193"@nl ;
	skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-nederlands>, <http://ilearn.ilabt.imec.be/vocab/curr2/s-schrijven>, <https://w3id.org/onderwijs-vlaanderen/id/structuur/lager-onderwijs> ;
	skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-4.8-3748772000 rdf:type skos:Concept ;
	skos:definition "4.8. De leerlingen kunnen suggesties geven voor het inrichten van hun eigen omgeving."@nl ;
	skos:hiddenLabel "4.8."@nl ;
	skos:inScheme onddoel:_scheme ;
	skos:prefLabel "basis-lager-4.8-3748772000"@nl ;
	skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-mens-en-maatschappij>, <http://ilearn.ilabt.imec.be/vocab/curr2/s-ruimte>, <http://ilearn.ilabt.imec.be/vocab/curr2/t-ruimtebeleving>, <https://w3id.org/onderwijs-vlaanderen/id/structuur/lager-onderwijs> ;
	skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-4.9-2415859818 rdf:type skos:Concept ;
	skos:definition "4.9. De leerlingen kunnen in de realiteit op een gepaste kaart een landelijke, stedelijke, toeristische en industrile omgeving herkennen en van elkaar onderscheiden."@nl ;
	skos:hiddenLabel "4.9."@nl ;
	skos:inScheme onddoel:_scheme ;
	skos:prefLabel "basis-lager-4.9-2415859818"@nl ;
	skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-mens-en-maatschappij>, <http://ilearn.ilabt.imec.be/vocab/curr2/s-ruimte>, <http://ilearn.ilabt.imec.be/vocab/curr2/t-ruimtelijke-ordening>, <https://w3id.org/onderwijs-vlaanderen/id/structuur/lager-onderwijs> ;
	skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-5-1741633790 rdf:type skos:Concept ;
	skos:definition "5 De leerlingen kunnen ICT gebruiken om eigen ideen creatief vorm te geven."@nl ;
	skos:hiddenLabel "5"@nl ;
	skos:inScheme onddoel:_scheme ;
	skos:prefLabel "basis-lager-5-1741633790"@nl ;
