	skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-5.4-3038188262 rdf:type skos:Concept ;
	skos:definition "5.4 De leerlingen kunnen bij de eindtermen luisteren, lezen, spreken en schrijven de volgende strategien inzetten: het resultaat beoordelen in het licht van het luister-, lees-, spreek- en schrijfdoel"@nl ;
	skos:hiddenLabel "5.4"@nl ;
	skos:inScheme onddoel:_scheme ;
	skos:prefLabel "basis-lager-5.4-3038188262"@nl ;
	skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-nederlands>, <http://ilearn.ilabt.imec.be/vocab/curr2/s-strategieen>, <https://w3id.org/onderwijs-vlaanderen/id/structuur/lager-onderwijs> ;
	skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-5.5-1449090885 rdf:type skos:Concept ;
	skos:definition "5.5 De leerlingen kunnen eenvoudige, audiovisuele informatie uit de eigen belevingswereld herkennen, onderzoeken en vergelijken."@nl ;
	skos:hiddenLabel "5.5"@nl ;
	skos:inScheme onddoel:_scheme ;
	skos:prefLabel "basis-lager-5.5-1449090885"@nl ;
