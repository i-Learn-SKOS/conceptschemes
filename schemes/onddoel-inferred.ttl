    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-sociale-vaardigheden-1.9 a skos:Concept ;
    skos:definition "1.9 De leerlingen kunnen ongelijk of onmacht toegeven, kritiek beluisteren en eruit leren."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-sociale-vaardigheden-1.9"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-sociale-vaardigheden>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-sociale-vaardigheden>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-sociale-vaardigheden-2 a skos:Concept ;
    skos:definition "2. De leerlingen kunnen in functionele situaties een aantal verbale en niet-verbale gespreksconventies naleven."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-sociale-vaardigheden-2"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-sociale-vaardigheden>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-sociale-vaardigheden>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-sociale-vaardigheden-3 a skos:Concept ;
    skos:definition "3. De leerlingen kunnen samenwerken met anderen, zonder onderscheid van sociale achtergrond, geslacht of etnische origine."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-sociale-vaardigheden-3"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-sociale-vaardigheden>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-sociale-vaardigheden>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-1.1 a skos:Concept ;
    skos:definition "1.1 De leerlingen kunnen tellen en terugtellen met eenheden, tweetallen, vijftallen en machten van tien."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-1.1"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-getallen>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-1.10 a skos:Concept ;
    skos:definition "1.10 De leerlingen zijn in staat tot een onmiddellijk geven van correcte resultaten bij optellen en aftrekken tot 10, bij tafels van vermenigvuldiging tot en met de tafels van 10 en de bijhorende deeltafels."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-1.10"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-getallen>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-1.11 a skos:Concept ;
    skos:definition "1.11 De leerlingen hebben inzicht in de relaties tussen de bewerkingen."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-1.11"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-getallen>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-1.12 a skos:Concept ;
    skos:definition "1.12 De leerlingen kunnen orde en regelmaat ontdekken in getallenpatronen onder meer om te komen tot de kenmerken van deelbaarheid door 2, 3, 5, 9, 10 en die te kunnen toepassen."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-1.12"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-getallen>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-1.13 a skos:Concept ;
    skos:definition """1.13 De leerlingen voeren opgaven uit het hoofd uit waarbij ze een doelmatige oplossingsweg kiezen op basis van inzicht in de eigenschappen van bewerkingen en in de structuur van getallen: 
- optellen en aftrekken tot honderd
- optellen en aftrekken met grote getallen met eindnullen
- vermenigvuldigen met en delen naar analogie met de tafels"""@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-1.13"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-getallen>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-1.14 a skos:Concept ;
    skos:definition "1.14 De leerlingen kunnen op concrete wijze de volgende eigenschappen van bewerkingen verwoorden en toepassen: van plaats wisselen, schakelen, splitsen en verdelen."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-1.14"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-getallen>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-1.15 a skos:Concept ;
    skos:definition "1.15 De leerlingen zijn in staat getallen af te ronden. De graad van nauwkeurigheid wordt bepaald door het doel van het afronden en door de context."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-1.15"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-getallen>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-1.16 a skos:Concept ;
    skos:definition "1.16 De leerlingen kunnen de uitkomst van een berekening bij benadering bepalen."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-1.16"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-getallen>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-1.17 a skos:Concept ;
    skos:definition "1.17 De leerlingen kunnen schatprocedures vinden bij niet exact bepaalde of niet exact te bepalen gegevens."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-1.17"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-getallen>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-1.18 a skos:Concept ;
    skos:definition "1.18 De leerlingen kunnen in eenvoudige getallen de gelijkwaardigheid tussen kommagetallen, breuken en procenten vaststellen en verduidelijken door omzettingen."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-1.18"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-getallen>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-1.19 a skos:Concept ;
    skos:definition "1.19 De leerlingen kunnen de delers van een natuurlijk getal (=100) vinden; zij kunnen van twee dergelijke getallen de (grootste) gemeenschappelijke deler(s) vinden."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-1.19"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-getallen>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-1.2 a skos:Concept ;
    skos:definition "1.2 De leerlingen kunnen de verschillende functies van natuurlijke getallen herkennen en verwoorden."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-1.2"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-getallen>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-1.20 a skos:Concept ;
    skos:definition "1.20 De leerlingen kunnen de veelvouden van een natuurlijk getal (<20) vinden, zij kunnen van twee dergelijke getallen het (kleinste) gemeenschappelijk veelvoud vinden."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-1.20"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-getallen>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-1.21 a skos:Concept ;
    skos:definition "1.21 De leerlingen zijn in staat in concrete situaties (onder meer tussen grootheden) eenvoudige verhoudingen vast te stellen, te vergelijken, hun gelijkwaardigheid te beoordelen en het ontbrekend verhoudingsgetal te berekenen."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-1.21"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-getallen>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-1.22 a skos:Concept ;
    skos:definition "1.22 De leerlingen kunnen eenvoudige breuken gelijknamig maken in functie van het optellen en aftrekken van breuken of in functie van het ordenen en het vergelijken van breuken."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-1.22"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-getallen>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-1.23 a skos:Concept ;
    skos:definition "1.23 De leerlingen kunnen in een zinvolle context eenvoudige breuken en kommagetallen optellen en aftrekken. In een zinvolle context kunnen zij eveneens een eenvoudige breuk vermenigvuldigen met een natuurlijk getal."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-1.23"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-getallen>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-1.24 a skos:Concept ;
    skos:definition """1.24 De leerlingen kennen de cijferalgoritmen. Zij kunnen cijferend vier hoofdbewerkingen uitvoeren met natuurlijke en met kommagetallen:
- optellen met max. 5 getallen: de som < 10 000 000;
- aftrekken: aftrektal < 10 000 000 en max. 8 cijfers;
- vermenigvuldigen: vermenigvuldiger bestaat uit max. 3 cijfers; het product = max. 8 cijfers (2 cijfers na de komma);
- delen: deler bestaat uit max. 3 cijfers; quotiënt max. 2 cijfers na de komma."""@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-1.24"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-getallen>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-1.25 a skos:Concept ;
    skos:definition "1.25 De leerlingen kunnen eenvoudige procentberekeningen maken met betrekking tot praktische situaties."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-1.25"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-getallen>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-1.26 a skos:Concept ;
    skos:definition "1.26 De leerlingen kunnen de zakrekenmachine doelmatig gebruiken voor de hoofdbewerkingen (zie ook 1.28)."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-1.26"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-getallen>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-1.27 a skos:Concept ;
    skos:definition "1.27 De leerlingen zijn in staat uitgevoerde bewerkingen te controleren, onder meer met de zakrekenmachine."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-1.27"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-getallen>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-1.28 a skos:Concept ;
    skos:definition "1.28 De leerlingen kunnen in contexten vaststellen welke wiskundige bewerkingen met betrekking tot getallen toepasselijk zijn en welke het meest aangewezen en economisch zijn."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-1.28"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-getallen>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-1.29s a skos:Concept ;
    skos:definition "1.29* De leerlingen zijn bereid verstandige zoekstrategieën aan te wenden die helpen bij het aanpakken van wiskundige problemen met betrekking tot getallen, meten, ruimtelijke oriëntatie en meetkunde."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-1.29s"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-getallen>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-1.3 a skos:Concept ;
    skos:definition "1.3 De leerlingen kennen de betekenis van: optellen, aftrekken, vermenigvuldigen, delen, veelvoud, deler, gemeenschappelijke deler, grootste gemeenschappelijke deler, kleinste gemeenschappelijk veelvoud, procent, som, verschil, product, quotiënt en rest. Zij kunnen correcte voorbeelden geven en kunnen verwoorden in welke situatie ze dit handig kunnen gebruiken."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-1.3"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-getallen>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-1.4 a skos:Concept ;
    skos:definition "1.4 De leerlingen herkennen in voorbeelden dat breuken kunnen uitgelegd worden als: een stuk (deel) van, een verhouding, een verdeling,een deling, een vermenigvuldigingsfactor (operator), een getal (met een plaats op een getallenlijn), weergave van het kans. De leerlingen kunnen volgende terminologie hanteren: stambreuk, teller, noemer, breukstreep, gelijknamig, gelijkwaardig."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-1.4"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-getallen>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-1.5 a skos:Concept ;
    skos:definition "1.5 De leerlingen kunnen natuurlijke getallen van maximaal 10 cijfers en kommagetallen (met 3 decimalen), eenvoudige breuken, eenvoudige procenten lezen, noteren, ordenen en op een getallenlijn plaatsen."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-1.5"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-getallen>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-1.6 a skos:Concept ;
    skos:definition "1.6 De leerlingen kunnen volgende symbolen benoemen, noteren en hanteren: = ≠ + - x . : / ÷ % en ( ) in bewerkingen."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-1.6"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-getallen>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-1.7 a skos:Concept ;
    skos:definition "1.7 De leerlingen kunnen door het geven van een paar voorbeelden uit hun eigen leefwereld en in hun leermateriaal aantonen dat doorheen de geschiedenis en ook in niet-westerse culturen andere wiskundige systemen met betrekking tot getallen werden en worden beoefend."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-1.7"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-getallen>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-1.8 a skos:Concept ;
    skos:definition "1.8 De leerlingen kunnen gevarieerde hoeveelheidsaanduidingen lezen en interpreteren."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-1.8"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-getallen>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-1.9 a skos:Concept ;
    skos:definition "1.9 De leerlingen kunnen in gesprekken de geleerde symbolen, terminologie, notatiewijzen en conventies gebruiken."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-1.9"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-getallen>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-2.1 a skos:Concept ;
    skos:definition "2.1 De leerlingen kennen de belangrijkste grootheden en maateenheden met betrekking tot lengte, oppervlakte, inhoud, gewicht(massa) tijd, snelheid, temperatuur en hoekgrootte en ze kunnen daarbij de relatie leggen tussen de grootheid en de maateenheid."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-2.1"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-meten>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-2.10 a skos:Concept ;
    skos:definition "2.10 De leerlingen kunnen concreet aangeven hoe de inhoud van een balk wordt bepaald."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-2.10"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-meten>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-2.11 a skos:Concept ;
    skos:definition "2.11 De leerlingen kunnen in reële situaties rekenen met geld en geldwaarden."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-2.11"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-meten>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-2.19 a skos:Concept ;
    skos:definition "2.19 De leerlingen kunnen kloklezen (analoge en digitale klokken). Zij kunnen tijdsintervallen berekenen en zij kennen de samenhang tussen seconden, minuten en uren."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-2.19"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-meten>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-2.2 a skos:Concept ;
    skos:definition "2.2 De leerlingen kennen de symbolen, notatiewijzen en conventies bij de gebruikelijke maateenheden en kunnen meetresultaten op veelzijdige wijze noteren en op verschillende wijze groeperen."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-2.2"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-meten>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-2.3 a skos:Concept ;
    skos:definition "2.3 De leerlingen kunnen veel voorkomende maten in verband brengen met betekenisvolle situaties."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-2.3"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-meten>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-2.4 a skos:Concept ;
    skos:definition "2.4 De leerlingen kunnen de functie van de begrippen \"schaal\" en \"gemiddelde\" aan de hand van concrete voorbeelden verwoorden."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-2.4"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-meten>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-2.5 a skos:Concept ;
    skos:definition "2.5 De leerlingen weten dat bij temperatuurmeting 0 °C het vriespunt is en weten dat de temperaturen beneden het vriespunt met een negatief getal worden aangeduid."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-2.5"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-meten>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-2.6 a skos:Concept ;
    skos:definition "2.6 De leerlingen kunnen allerlei verbanden, patronen en structuren tussen en met grootheden en maatgetallen inzien en ze kunnen betekenisvolle herleidingen uitvoeren."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-2.6"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-meten>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-2.7 a skos:Concept ;
    skos:definition "2.7 De leerlingen kunnen met de gebruikelijke maateenheden betekenisvolle herleidingen uitvoeren."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-2.7"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-meten>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-2.8 a skos:Concept ;
    skos:definition "2.8 De leerlingen kunnen schatten met behulp van referentiepunten."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-2.8"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-meten>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-2.9 a skos:Concept ;
    skos:definition "2.9 De leerlingen kunnen op een concrete wijze aangeven hoe ze de oppervlakte en de omtrek van een willekeurige, vlakke figuur en van een veelhoek kunnen bepalen."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-2.9"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-meten>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-3.1 a skos:Concept ;
    skos:definition "3.1 De leerlingen kunnen begrippen en notaties waarmee de ruimte meetkundig wordt bepaald aan de hand van concrete voorbeelden verklaren."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-3.1"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-meetkunde>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-3.2 a skos:Concept ;
    skos:definition """3.2 De leerlingen kunnen op basis van volgende eigenschappen de volgende meetkundige objecten herkennen en benoemen :
a) in het vlak : punten, lijnen, hoeken en vlakke figuren (driehoeken, vierhoeken, cirkels);
b) in de ruimte : veelvlakken (kubus, balk, piramide) en bol en cilinder."""@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-3.2"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-meetkunde>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-3.3 a skos:Concept ;
    skos:definition "3.3 De leerlingen kunnen de symbolen van de loodrechte stand en van de evenwijdigheid lezen en noteren."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-3.3"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-meetkunde>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-3.4 a skos:Concept ;
    skos:definition "3.4 De leerlingen kunnen de verschillende soorten hoeken classificeren en de verschillende soorten vierhoeken classificeren op grond van zijden en hoeken. Zij kunnen deze ook concreet vormgeven."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-3.4"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-meetkunde>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-3.5 a skos:Concept ;
    skos:definition "3.5 De leerlingen kunnen met een passer een cirkel tekenen."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-3.5"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-meetkunde>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-3.6 a skos:Concept ;
    skos:definition "3.6 De leerlingen kunnen de begrippen symmetrie, gelijkvormigheid en gelijkheid ontdekken in de realiteit. Ze kunnen zelf eenvoudige geometrische figuren maken."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-3.6"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-meetkunde>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-3.7 a skos:Concept ;
    skos:definition """3.7 De leerlingen zijn in staat:
- zich ruimtelijk te oriënteren op basis van plattegronden, kaarten, foto's en gegevens over afstand en richting; 
- zich in de ruimte mentaal te verplaatsen en te verwoorden wat ze dan zien."""@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-3.7"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-meetkunde>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-4.1 a skos:Concept ;
    skos:definition "4.1 De leerlingen kunnen met concrete voorbeelden aantonen dat er voor hetzelfde wiskundig probleem met betrekking tot getallen, meten, meetkunde en ruimtelijke oriëntatie, soms meerdere oplossingswegen zijn en soms zelfs meerdere oplossingen mogelijk zijn afhankelijk van de wijze waarop het probleem wordt opgevat."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-4.1"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-stratprobv>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wiskunde-4.2 a skos:Concept ;
    skos:definition "4.2 De leerlingen zijn in staat om de geleerde begrippen, inzichten, procedures, met betrekking tot getallen, meten en meetkunde, zoals in de respectievelijke eindtermen vermeld, efficiënt te hanteren in betekenisvolle toepassingssituaties, zowel binnen als buiten de klas."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wiskunde-4.2"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wiskunde>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-stratprobv>,
