    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-mens-en-maatschappij>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-tijd>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/t-historische-tijd>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-mens-en-maatschappij-3.7 a skos:Concept ;
    skos:definition "3.7. De leerlingen kennen de grote periodes uit de geschiedenis en ze kunnen duidelijke historische elementen in hun omgeving en belangrijke historische figuren en gebeurtenissen waarmee ze kennis maken, situeren in de juiste tijdsperiode aan de hand van een tijdband."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-mens-en-maatschappij-3.7"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-mens-en-maatschappij>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-tijd>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/t-historische-tijd>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-mens-en-maatschappij-3.8 a skos:Concept ;
    skos:definition "3.8. De leerlingen kunnen aan de hand van een voorbeeld illustreren dat een actuele toestand, die voor kinderen herkenbaar is, en die door de geschiedenis beïnvloed werd, vroeger anders was en in de loop der tijden evolueert."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-mens-en-maatschappij-3.8"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-mens-en-maatschappij>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-tijd>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/t-historische-tijd>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-mens-en-maatschappij-3.9 a skos:Concept ;
    skos:definition "3.9. De leerlingen tonen belangstelling voor het verleden, heden en de toekomst, hier en elders."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-mens-en-maatschappij-3.9"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-mens-en-maatschappij>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-tijd>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/t-historische-tijd>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-mens-en-maatschappij-4.1 a skos:Concept ;
    skos:definition "4.1. De leerlingen kunnen aan elkaar een te volgen weg tussen twee plaatsen in de eigen gemeente of stad beschrijven. Ze kunnen deze reisweg ook aanduiden op een plattegrond."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-mens-en-maatschappij-4.1"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-mens-en-maatschappij>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-ruimte>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/t-orientatie-en-kaartvaardigheid>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-mens-en-maatschappij-4.10 a skos:Concept ;
    skos:definition "4.10. De leerlingen kunnen hun eigen streek en twee andere streken in België situeren op een kaart en de relatie beschrijven tussen de omgeving en aspecten van het dagelijks leven van de mensen."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-mens-en-maatschappij-4.10"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-mens-en-maatschappij>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-ruimte>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/t-ruimtelijke-ordening>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-mens-en-maatschappij-4.11 a skos:Concept ;
    skos:definition "4.11. De leerlingen kunnen aspecten van het dagelijks leven in een land van een ander cultuurgebied vergelijken met het eigen leven."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-mens-en-maatschappij-4.11"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-mens-en-maatschappij>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-ruimte>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/t-ruimtelijke-ordening>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-mens-en-maatschappij-4.12 a skos:Concept ;
    skos:definition "4.12. De leerlingen kunnen in een landschap gericht waarnemen en ze kunnen op een eenvoudige wijze onderzoeken waarom het er zo uitziet."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-mens-en-maatschappij-4.12"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-mens-en-maatschappij>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-ruimte>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/t-algemene-vaardigheden-ruimte>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-mens-en-maatschappij-4.13 a skos:Concept ;
    skos:definition "4.13. De leerlingen kunnen een atlas raadplegen en kunnen enkele soorten kaarten hanteren gebruik makend van de legende, windrichting en schaal."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-mens-en-maatschappij-4.13"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-mens-en-maatschappij>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-ruimte>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/t-algemene-vaardigheden-ruimte>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-mens-en-maatschappij-4.14 a skos:Concept ;
    skos:definition "4.14. De leerlingen kunnen de gevaarlijke verkeerssituaties in de ruimere schoolomgeving lokaliseren."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-mens-en-maatschappij-4.14"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-mens-en-maatschappij>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-ruimte>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/t-verkeer-en-mobiliteit>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-mens-en-maatschappij-4.15 a skos:Concept ;
    skos:definition "4.15. De leerlingen beschikken over voldoende reactiesnelheid, evenwichtsbehoud en gevoel voor coördinatie en ze kennen de verkeersregels voor fietsers en voetgangers, om zich zelfstandig en veilig te kunnen verplaatsen langs een voor hen vertrouwde route."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-mens-en-maatschappij-4.15"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-mens-en-maatschappij>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-ruimte>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/t-verkeer-en-mobiliteit>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-mens-en-maatschappij-4.16 a skos:Concept ;
    skos:definition "4.16. De leerlingen tonen zich in hun gedrag bereid rekening te houden met andere weggebruikers."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-mens-en-maatschappij-4.16"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-mens-en-maatschappij>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-ruimte>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/t-verkeer-en-mobiliteit>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-mens-en-maatschappij-4.17 a skos:Concept ;
    skos:definition "4.17. De leerlingen kennen de belangrijkste gevolgen van het groeiende autogebruik en kunnen de voor- en nadelen van mogelijke alternatieven vergelijken."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-mens-en-maatschappij-4.17"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-mens-en-maatschappij>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-ruimte>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/t-verkeer-en-mobiliteit>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-mens-en-maatschappij-4.18 a skos:Concept ;
    skos:definition "4.18. De leerlingen kunnen een eenvoudige route uitstippelen met het openbaar vervoer."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-mens-en-maatschappij-4.18"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-mens-en-maatschappij>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-ruimte>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/t-verkeer-en-mobiliteit>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-mens-en-maatschappij-4.2 a skos:Concept ;
    skos:definition "4.2. De leerlingen kunnen aan de hand van een kaart de afstand tussen twee plaatsen in Vlaanderen berekenen en beschrijven."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-mens-en-maatschappij-4.2"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-mens-en-maatschappij>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-ruimte>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/t-orientatie-en-kaartvaardigheid>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-mens-en-maatschappij-4.3 a skos:Concept ;
    skos:definition "4.3. De leerlingen kunnen in een praktische toepassingssituatie op een gepaste kaart en op de globe evenaar, de polen, de oceanen , de landen van de Europese Unie en de werelddelen opzoeken en aanwijzen."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-mens-en-maatschappij-4.3"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-mens-en-maatschappij>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-ruimte>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/t-orientatie-en-kaartvaardigheid>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-mens-en-maatschappij-4.4 a skos:Concept ;
    skos:definition "4.4. De leerlingen kunnen bij een oriëntatie in de werkelijkheid de windstreken (hoofd- en tussenrichtingen) bepalen aan de hand van de zonnestand of een kompas."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-mens-en-maatschappij-4.4"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-mens-en-maatschappij>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-ruimte>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/t-orientatie-en-kaartvaardigheid>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-mens-en-maatschappij-4.5 a skos:Concept ;
    skos:definition "4.5. De leerlingen kunnen begrippen zoals wijk, gehucht, dorp, deelgemeente, fusiegemeente, stad, provincie, gemeenschap, land en continent in een juiste context gebruiken."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-mens-en-maatschappij-4.5"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-mens-en-maatschappij>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-ruimte>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/t-orientatie-en-kaartvaardigheid>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-mens-en-maatschappij-4.6 a skos:Concept ;
    skos:definition "4.6. De leerlingen hebben een voorstelling van de kaart van Vlaanderen en van België zodat ze in een praktische toepassingssituatie de gemeenschappen, de provincies en de provinciehoofdplaatsen kunnen aanwijzen."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-mens-en-maatschappij-4.6"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-mens-en-maatschappij>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-ruimte>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/t-orientatie-en-kaartvaardigheid>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-mens-en-maatschappij-4.7 a skos:Concept ;
    skos:definition "4.7. De leerlingen kunnen aan de hand van een concreet voorbeeld het verschil tussen beleefde en absolute afstand illustreren."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-mens-en-maatschappij-4.7"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-mens-en-maatschappij>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-ruimte>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/t-ruimtebeleving>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-mens-en-maatschappij-4.8 a skos:Concept ;
    skos:definition "4.8. De leerlingen kunnen suggesties geven voor het inrichten van hun eigen omgeving."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-mens-en-maatschappij-4.8"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-mens-en-maatschappij>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-ruimte>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/t-ruimtebeleving>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-mens-en-maatschappij-4.9 a skos:Concept ;
    skos:definition "4.9. De leerlingen kunnen in de realiteit op een gepaste kaart een landelijke, stedelijke, toeristische en industriële omgeving herkennen en van elkaar onderscheiden."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-mens-en-maatschappij-4.9"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-mens-en-maatschappij>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-ruimte>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/t-ruimtelijke-ordening>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wetenschap-en-techniek-1.1 a skos:Concept ;
    skos:definition "1.1. De leerlingen kunnen gericht waarnemen met alle zintuigen en kunnen waarnemingen op een systematische wijze noteren:"@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wetenschap-en-techniek-1.1"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wetenschap-en-techniek>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-natuur>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/t-algemene-vaardigheden>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wetenschap-en-techniek-1.10 a skos:Concept ;
    skos:definition "1.10. De leerlingen kunnen lichamelijke veranderingen die ze bij zichzelf en leeftijdsgenoten waarnemen, herkennen als normale aspecten in hun ontwikkeling;"@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wetenschap-en-techniek-1.10"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wetenschap-en-techniek>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-natuur>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/t-levende-en-niet-levende-natuur>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wetenschap-en-techniek-1.11 a skos:Concept ;
    skos:definition "1.11. De leerlingen kunnen de weerselementen op een bepaald moment en over een beperkte periode, meten, vergelijken en die weersituatie beschrijven;"@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wetenschap-en-techniek-1.11"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wetenschap-en-techniek>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-natuur>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/t-levende-en-niet-levende-natuur>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wetenschap-en-techniek-1.12 a skos:Concept ;
    skos:definition "1.12. De leerlingen kunnen het verband illustreren tussen de leefgewoonten van mensen en het klimaat waarin ze leven;"@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wetenschap-en-techniek-1.12"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wetenschap-en-techniek>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-natuur>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/t-levende-en-niet-levende-natuur>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wetenschap-en-techniek-1.13 a skos:Concept ;
    skos:definition "1.13. De leerlingen kunnen tonen hoe de aarde om de eigen as draait, welk gevolg dit heeft voor het dag- en nachtritme in de eigen omgeving en hoe de aarde, de zon en de maan ten opzichte van elkaar bewegen;"@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-wetenschap-en-techniek-1.13"@nl ;
    skos:related <http://ilearn.ilabt.imec.be/vocab/curr2/l-wetenschap-en-techniek>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/s-natuur>,
        <http://ilearn.ilabt.imec.be/vocab/curr2/t-levende-en-niet-levende-natuur>,
        <http://ilearn.ilabt.imec.be/vocab/ondniv/basis-lager> ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-wetenschap-en-techniek-1.14 a skos:Concept ;
    skos:definition "1.14. De leerlingen kunnen van courante materialen uit hun omgeving enkele eigenschappen aantonen;"@nl ;
