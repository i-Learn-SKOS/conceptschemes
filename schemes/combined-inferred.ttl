    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-muzische-vorming-2.5 a skos:Concept ;
    skos:definition "2.5 De leerlingen kunnen vanuit het eigen muzikaal aanvoelen praten over het zingen en het musiceren."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-muzische-vorming-2.5"@nl ;
    skos:related curr2:l-muzische-vorming,
        curr2:s-muziek,
        ondniv:basis-lager ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-muzische-vorming-3.1 a skos:Concept ;
    skos:definition "3.1 De leerlingen kunnen genieten van een gevarieerd aanbod van voor hen bestemde culturele activiteiten."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-muzische-vorming-3.1"@nl ;
    skos:related curr2:l-muzische-vorming,
        curr2:s-drama,
        ondniv:basis-lager ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-muzische-vorming-3.2 a skos:Concept ;
    skos:definition "3.2 De leerlingen kunnen spelvormen waarnemen en inzien dat de juiste verhouding tussen woord en beweging de expressie kan vergroten."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-muzische-vorming-3.2"@nl ;
    skos:related curr2:l-muzische-vorming,
        curr2:s-drama,
        ondniv:basis-lager ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-muzische-vorming-3.3 a skos:Concept ;
    skos:definition "3.3 De leerlingen kunnen geconcentreerd luisteren naar een gesproken tekst (verteld of voorgelezen) en die mondeling, schriftelijk, beeldend of dramatisch weergeven."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-muzische-vorming-3.3"@nl ;
    skos:related curr2:l-muzische-vorming,
        curr2:s-drama,
        ondniv:basis-lager ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-muzische-vorming-3.4 a skos:Concept ;
    skos:definition "3.4 De leerlingen kunnen spelvormen in een sociale en maatschappelijke context hanteren."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-muzische-vorming-3.4"@nl ;
    skos:related curr2:l-muzische-vorming,
        curr2:s-drama,
        ondniv:basis-lager ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-muzische-vorming-3.5 a skos:Concept ;
    skos:definition "3.5 De leerlingen kunnen ervaringen, gevoelens, ideen, fantasien ... uiten in spel."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-muzische-vorming-3.5"@nl ;
    skos:related curr2:l-muzische-vorming,
        curr2:s-drama,
        ondniv:basis-lager ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-muzische-vorming-3.6 a skos:Concept ;
    skos:definition "3.6 De leerlingen kunnen een aan de speelsituatie aangepaste en aangename spreektechniek ontwikkelen (articulatie, adembeheersing, tempo, toonhoogte) en verschillende verbale en non-verbale spelvormen improviseren."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-muzische-vorming-3.6"@nl ;
    skos:related curr2:l-muzische-vorming,
        curr2:s-drama,
        ondniv:basis-lager ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-muzische-vorming-3.7 a skos:Concept ;
    skos:definition "3.7 De leerlingen kunnen genieten van, praten over en kritisch staan tegenover het eigen spel en dat van anderen, de keuze van spelvormen, onderwerpen, de beleving."@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-muzische-vorming-3.7"@nl ;
    skos:related curr2:l-muzische-vorming,
        curr2:s-drama,
        ondniv:basis-lager ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-muzische-vorming-4.1 a skos:Concept ;
    skos:definition "4.1 De leerlingen kunnen genieten van lichaamstaal, beweging en dans."@nl ;
