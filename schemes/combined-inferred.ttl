    skos:prefLabel "basis-lager-muzische-vorming-4.2"@nl ;
    skos:related curr2:l-muzische-vorming,
        curr2:s-beweging,
        ondniv:basis-lager ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-muzische-vorming-4.3 a skos:Concept ;
    skos:definition """4.3 De leerlingen kunnen samenwerken met anderen: 
- om een eenvoudig dansverloop op te bouwen 
- om al improviserend te reageren op elkaars beweging."""@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-muzische-vorming-4.3"@nl ;
    skos:related curr2:l-muzische-vorming,
        curr2:s-beweging,
        ondniv:basis-lager ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-muzische-vorming-4.4 a skos:Concept ;
    skos:definition """4.4 De leerlingen kunnen bewegen op een creatieve manier en daarbij n of meerdere basiselementen van de beweging bespelen: 
- tijd; 
- kracht; 
- ruimte; 
- lichaamsmogelijkheden"""@nl ;
    skos:inScheme onddoel:_scheme ;
    skos:prefLabel "basis-lager-muzische-vorming-4.4"@nl ;
    skos:related curr2:l-muzische-vorming,
        curr2:s-beweging,
        ondniv:basis-lager ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-muzische-vorming-4.5 a skos:Concept ;
    skos:definition "4.5 De leerlingen kunnen nieuwe dansen ontwerpen met eenvoudige passen en figuren."@nl ;
