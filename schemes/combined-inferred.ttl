    skos:prefLabel "basis-lager-muzische-vorming-4.1"@nl ;
    skos:related curr2:l-muzische-vorming,
        curr2:s-beweging,
        ondniv:basis-lager ;
    skos:topConceptOf onddoel:_scheme .

onddoel:basis-lager-muzische-vorming-4.2 a skos:Concept ;
    skos:definition "4.2 De leerlingen kunnen een eenvoudig bewegingsverhaal opbouwen met als vertrekpunt iets wat gehoord, gezien, gelezen, gevoeld of meegemaakt wordt."@nl ;
